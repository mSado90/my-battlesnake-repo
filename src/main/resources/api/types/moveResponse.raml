#%RAML 1.0 DataType
type: object
displayName: Move Response
properties:
  move:
    description: Your Battlesnake's move for this turn. 
    enum:
      - up
      - down
      - left
      - right
  shout:
    type: string
    required: false
    description: An optional message sent to all other Battlesnakes on the next turn
    maxLength: 256