#%RAML 1.0 DataType
type: object
displayName: Royale
properties:
  shrinkEveryNTurns:
    type: number
    displayName: Shrink Every N Turns
    required: true
    description: |
      In Royale mode, the number of turns between generating new hazards (shrinking the safe board space).
    examples:
      Ex1: 5
examples:
  Ex1:
    shrinkEveryNTurns: 5